    var pathCss = '/assets/vendor/css/advertisement/gujAd.css?t=1476116116';
    var pathJs = '/assets/vendor/js/advertisement/gujAd_neu.js?t=1476116116';

    // wenn css und js Pfad gleich sind wenn nicht entsprechend Ändern '\[foldername]\' var pathJs = pathCss;
    var siteName = 'neu.fotocommunity.de'; // wird von EMS definiert
    
    if (window.location.search.indexOf('gujAd=1') > -1) {
        pathCss = 'http://preview.adsatellite.guj.de/satellite.php/get-unminified-file-content/1/' + siteName + '/';
        pathJs  = pathCss;
        
        document.write('<scr'+'ipt type="text/javascript" src="'+ pathJs +'gujAd.js"></scr'+'ipt>');
        document.write('<link rel="stylesheet" type="text/css" media="screen" href="'+ pathCss + 'gujAd.css" />');
        
    } else {
        document.write('<scr'+'ipt type="text/javascript" src="'+ pathJs +'"></scr'+'ipt>');
        document.write('<link rel="stylesheet" type="text/css" media="screen" href="'+ pathCss + '" />');
    }
        
</script>

<script type="text/javascript">

    // Aufruf muss in separatem Script-Tag stehen
    if(typeof GujAd !== 'undefined') {

                    var zoneLabel = 'displayseiten';
        
        if ($(window).width() >= 1280) {
            zoneName = zoneLabel + '/large_' + zoneLabel;
        } else if ($(window).width() >= 1120) {
            zoneName = zoneLabel + '/small_' + zoneLabel;
        } else {
            zoneName = '_noad';
        }

        GujAd.getClient().setZone(zoneName);
    }

        
    
</script>                
